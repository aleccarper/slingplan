<div class='page-header'>
  <%= link_to "<i class='left fa fa-arrow-left'></i>Back".html_safe, admin_locations_path, class: 'left btn btn-back-to-locations' %>
  <div class='title'>Claimed</div>
</div>

<div class="claims">
  <% if @claims.length > 0 %>
    <ul>
      <% for claim in @claims %>
        <%= location_claim(claim) %>
      <% end %>
    </ul>
  <% else %>
    <div class='nothing-here'>You have no ongoing claims.</div>
  <% end %>
</div>

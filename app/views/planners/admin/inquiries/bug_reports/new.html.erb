<%= render partial: 'inquiry_header' %>

<div class='inner bug_reports'>
  <h1>New Bug Report</h1>

  <%= responsive_form_for @bug_report, url: planners_admin_inquiries_bug_reports_path do |f| %>
    <%= f.text_field :subject %>
    <%= f.text_area :body %>

    <div class='submit'>
      <%= back_to_planner_inquiries %>
      <%= submit_tag 'Create', class: 'btn f-right' %>
    </div>
  <% end %>
</div>
